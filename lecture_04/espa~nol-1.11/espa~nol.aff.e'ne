    A L I R	> -R, STEIS		# salir > salisteis
    E B I R	> -R, STEIS		# concebir > concibieron
    E C I R	> -ECIR, IJISTEIS	# decir > dijisteis
    E D I R	> -R, STEIS		# pedir > pidieron
    E G I R	> -R, STEIS		# elegir > eligieron
    E M I R	> -R, STEIS		# gemir > gimieron
    [EO] 'I R	> -R, STEIS		# re'ir > re'isteis
    E 'N I R	> -R, STEIS		# ce'nir > ce'nisteis
    E N C H I R	> -R, STEIS		# henchir > hinchieron
