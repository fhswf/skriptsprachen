    [^D] U C I R > -R, ESEN
    [^G] U I R 	> -IR, YERAN		# huir > huyesen
    [^G] U I R 	> -IR, YESEN		# huir > huyeran
    G \"U I R	> -\"UIR, UYESEN	# arg"uir > arguyeran o arguyesen
    G \"U I R	> -\"UIR, UYERAN
    U L L I R	> -IR, ERAN		# mullir > mulleran o mullesen
    U L L I R	> -IR, ESEN
    U 'N I R	> -IR, ERAN		# gruñir > gruñeran o gruñesen
    U 'N I R	> -IR, ESEN
    E H U N D I R > -R, ERAN		# rehundir > rehundieran o rehundiesen
    E H U N D I R > -R, ESEN
    E R G U I R	> -ERGUIR, IRGUIERAN	# erguir > irguieran
    E R G U I R	> -ERGUIR, IRGUIESEN	# erguir > irguiesen

# SUBJUNTIVO FUTURO
    A N D A R	> -AR, UVIERE		# andar > anduviere
    [^A] N D A R> -R, RE		# recomendar > recomendare
    [^N] D A R	> -R, RE		# recordar > recordare
    [^D] A R	> -R, RE		# amar > amare
    [^FH] A C E R > -ER, IERE		# nacer > naciere
    [FH] A C E R > -ACER, ICIERE	# hacer > hiciere
    A B E R	> -ABER, UPIERE		# saber > supiere
    A L E R	> -ER, IERE		# valer > valiere
    C A E R	> -ER, YERE		# caer > cayere
    E [CD] E R	> -ER, IERE		# parecer > pareciere
    [EO] E R	> -ER, YERE		# leer > leyere
    E N D E R	> -ER, IERE		# tender > tendiere
    E N E R	> -ENER, UVIERE		# tener > tuviere
    E R [DNT] E R > -ER, IERE		# cerner > cerniere
    E R E R	> -ERER, ISIERE		# querer > quisiere
    O [CLV] E R	> -ER, IERE		# conocer > conociere
    O D E R	> -ODER, UDIERE		# poder > pudiere
    O L V E R	> -ER, IERE		# volver > volviere
    O N E R	> -ONER, USIERE		# poner > pusiere
    O R [CD] E R > -ER, IERE		# torcer > torciere
    R A E R	> -ER, JERE		# traer > trajere
    A L I R	> -R, ERE		# salir > saliere
    E B I R	> -EBIR, IBIERE		# concebir > concibiere
    E C I R	> -ECIR, IJERE		# decir > dijere
    E D I R	> -EDIR, IDIERE		# pedir > pidiere
    E G I R	> -EGIR, IGIERE		# elegir > eligiere
    E M I R	> -EMIR, IMIERE		# gemir > gimiere
    E 'I R	> -E'IR, IERE		# re'ir > riere
    E 'N I R	> -E'NIR, I'NERE	# ce'nir > ci'nere
    E G U I R	> -EGUIR, IGUIERE	# seguir > siguiere
    E N C H I R	> -ENCHIR, INCHIERE	# henchir > hinchiere
    E N D I R	> -ENDIR, INDIERE	# rendir > rindiere
    E N I R	> -ENIR, INIERE		# venir > viniere
    E N T I R	> -ENTIR, INTIERE	# sentir > sintiere
    E R I R	> -ERIR, IRIERE		# herir > hiriere
    E R N I R	> -R, ERE		# discernir > discerniere
    E R T I R	> -ERTIR, IRTIERE	# invertir > invirtiere
    E R V I R	> -ERVIR, IRVIERE	# servir > sirviere
    E S T I R	> -ESTIR, ISTIERE	# sentir > vistiere
    E U N I R	> -R, ERE		# reunir > reuniere
    E T I R	> -ETIR, ITIERE		# derretir > derritiere
    I R I R	> -IRIR, IRIERE		# adquirir > adquiriere
    O H I B I R > -R, ERE		# prohibir > prohibiere
    O 'I R	> -'IR, YERE		# o'ir > oyere
    O R I R	> -ORIR, URIERE		# morir > muriere
    O R M I R	> -ORMIR, URMIERE	# dormir > durmiere
    D U C I R	> -CIR, JERE		# producir > produjere
    [^D] U C I R > -R, ERE		# lucir > luciere
    [^G] U I R	> -IR, YERE		# huir > huyere
    G \"U I R	> -\"UIR, UYERE		# arg"uir > arguyere
    U L L I R	> -IR, ERE		# mullir > mullere
    U 'N I R	> -IR, ERE		# gruñir > gruñere
    E H U N D I R > -R, ERE		# rehundir > rehundiere
    E R G U I R	> -ERGUIR, IRGUIERE	# erguir > irguiere

