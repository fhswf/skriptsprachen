    E N D E R	> -ER, ISTEIS		# tender > tendisteis
    E N E R	> -ENER, UVISTEIS	# tener > tuvisteis
    E R [DNT] E R > -ER, ISTEIS		# perder > perdisteis
    E R E R	> -ERER, ISISTEIS	# querer > quisisteis
    O [CLV] E R	> -ER, ISTEIS		# conocer > conocisteis
    O D E R	> -ODER, UDISTEIS	# poder > pudisteis
    O L V E R	> -ER, ISTEIS		# volver > volvisteis
    O N E R	> -ONER, USISTEIS	# poner > pusisteis
    O R [CD] E R > -ER, ISTEIS		# torcer > torcisteis
