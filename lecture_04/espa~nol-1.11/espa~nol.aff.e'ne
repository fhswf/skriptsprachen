# IMPERATIVO
	# La segunda persona de imperativo es la misma que la tercera
	# de indicativo, salvo excepciones (hacer, poner)


    [FH] A C E R > -CER, Z		# hacer > haz
#    E C I R	> -ECIR, I		# decir > di
    [AEI'I] R	> -R, D			# enviar > enviad
    . T E N E R	> -ENER,'EN		# retener > ret'en
    . P O N E R	> -ONER,'ON		# reponer > rep'on
    . V E N I R	> -ENIR,'EN		# prevenir > prev'en

flag *Y: # Participios y Gerundio. Verbos Irregulares.
    [AEO] E R	> -ER, 'IDO		# caer > ca'ido
    [AEO] E R	> -ER, 'IDOS		# comer > comidos
    [AEO] E R 	> -ER, 'IDA		# comer > comida
    [AEO] E R	> -ER, 'IDAS		# comer > comidas
    [FH] A C E R > -ACER, ECHO		# hacer > hecho
    [FH] A C E R > -ACER, ECHOS		# hacer > hechos
    [FH] A C E R > -ACER, ECHA		# hacer > hecha
    [FH] A C E R > -ACER, ECHAS		# hacer > hechas
    O D E R	> -ER, IDO		# poder > podido
    O D E R	> -ER, IDOS		# poder > podidos
    O D E R	> -ER, IDA		# poder > podida
    O D E R	> -ER, IDAS		# poder > podidas
    O L V E R	> -OLVER, UELTO		# volver > vuelto
    O L V E R	> -OLVER, UELTOS	# volver > vueltos
    O L V E R	> -OLVER, UELTA		# volver > vuelta
    O L V E R	> -OLVER, UELTAS	# volver > vueltas
    O N E R	> -ONER, UESTO		# poner > puesto
    O N E R	> -ONER, UESTOS		# poner > puestos
    O N E R	> -ONER, UESTA		# poner > puesta
    O N E R	> -ONER, UESTAS		# poner > puestas
    A 'N E R	> -ER, IDO		# tañer > tañido
    A 'N E R	> -ER, IDA		# tañer > tañida
    A 'N E R	> -ER, IDOS		# tañer > tañidos
    A 'N E R	> -ER, IDAS		# tañer > tañidas

    A B R I R	> -RIR, IERTO		# abrir > abierto
    A B R I R	> -RIR, IERTOS		# abrir > abiertos
    A B R I R	> -RIR, IERTA		# abrir > abierta
    A B R I R	> -RIR, IERTAS		# abrir > abiertas
    E [BDGMNRT'N] I R > -R, DO		# pedir > pedido
    E [BDGMNRT'N] I R > -R, DOS		# pedir > pedidos
    E [BDGMNRT'N] I R > -R, DA		# pedir > pedida
    E [BDGMNRT'N] I R > -R, DAS		# pedir > pedidas
    E C I R	> -ECIR, ICHO		# decir > dicho
    E C I R	> -ECIR, ICHOS		# decir > dichos
    E C I R	> -ECIR, ICHA		# decir > dicha
    E C I R	> -ECIR, ICHAS		# decir > dichas
    [EO] 'I R	> -R, DO		# re'ir > re'ido
    [EO] 'I R	> -R, DOS		# re'ir > re'idos
    [EO] 'I R	> -R, DA		# re'ir > re'ida
    [EO] 'I R	> -R, DAS		# re'ir > re'idas
    E N C H I R	> -R, DO		# henchir > henchido
    E N C H I R	> -R, DOS		# henchir > henchidos
    E N C H I R	> -R, DA		# henchir > henchida
    E N C H I R	> -R, DAS		# henchir > henchidas
    E N [DT] I R > -R, DO		# rendir > rendido
    E N [DT] I R > -R, DOS		# rendir > rendidos
    E N [DT] I R > -R, DA		# rendir > rendida
    E N [DT] I R > -R, DAS		# rendir > rendidas
    E R [TV] I R > -R, DO		# invertir > invertido
    E R [TV] I R > -R, DOS		# invertir > invertidos
    E R [TV] I R > -R, DA		# invertir > invertida
    E R [TV] I R > -R, DAS		# invertir > invertidas
    E S T I R	> -R, DO		# vestir > vestido
    E S T I R	> -R, DOS		# vestir > vestidos
    E S T I R	> -R, DA		# vestir > vestida
    E S T I R	> -R, DAS		# vestir > vestidas
    I B I R	> -BIR, TO		# escribir > escrito
    I B I R	> -BIR, TOS		# escribir > escritos
    I B I R	> -BIR, TA		# escribir > escrita
    I B I R	> -BIR, TAS		# escribir > escritas
    O R I R	> -ORIR, UERTO		# morir > muerto
    O R I R	> -ORIR, UERTOS		# morir > muertos
    O R I R	> -ORIR, UERTA		# morir > muerta
    O R I R	> -ORIR, UERTAS		# morir > muertas
    O R M I R	> -R, DO		# dormir > dormido
    O R M I R	> -R, DOS		# dormir > dormidos
    O R M I R	> -R, DA		# dormir > dormida
    O R M I R	> -R, DAS		# dormir > dormidas
    U B R I R	> -RIR, IERTO		# dormir > dormido
    U B R I R	> -RIR, IERTOS		# dormir > dormidos
    U B R I R	> -RIR, IERTA		# dormir > dormida
    U B R I R	> -RIR, IERTAS		# dormir > dormidas
    ["UU] I R	> -R, DO		# huir > huido
    ["UU] I R	> -R, DA		# huir > huidos
    ["UU] I R	> -R, DOS		# huir > huida
    ["UU] I R	> -R, DAS		# huir > huidas
    U L L I R	> -R, DO		# mullir > mullido
    U L L I R	> -R, DA		# mullir > mullida
    U L L I R	> -R, DOS		# mullir > mullidos
    U L L I R	> -R, DAS		# mullir > mullidas
    U 'N I R	> -R, DO		# gruñir > gruñido
    U 'N I R	> -R, DA		# gruñir > gruñida
    U 'N I R	> -R, DOS		# gruñir > gruñidos
    U 'N I R	> -R, DAS		# gruñir > gruñidas

    [FH] A C E R > -ER, IENDO		# hacer > haciendo
    [AEO] E R 	> -ER, YENDO		# caer > cayendo
    O D E R	> -ODER, UDIENDO	# poder > pudiendo
    O L V E R	> -ER, IENDO		# volver > volviendo
    O N E R	> -ER, IENDO		# poner > poniendo
    O M P E R	> -ER, IENDO		# romper > rompiendo
    A 'N E R	> -R, NDO		# tañer > tañendo

    A B R I R	> -R, ENDO		# abrir > abriendo
    E B I R	> -EBIR, IBIENDO	# concebir > concibiendo
    E C I R	> -ECIR, ICIENDO	# decir > diciendo
    E D I R	> -EDIR, IDIENDO	# pedir > pidiendo
    E G I R	> -EGIR, IGIENDO	# elegir > eligiendo
    E M I R	> -EMIR, IMIENDO	# gemir > gimiendo
    E 'I R	> -E'IR, IENDO		# re'ir > riendo
    E 'N I R	> -E'NIR, I'NENDO	# ce'nir > ci'nendo
    E N C H I R	> -ENCHIR, INCHIENDO	# henchir > hinchiendo
    E N D I R	> -ENDIR, INDIENDO	# rendir > rindiendo
    E N I R	> -ENIR, INIENDO	# venir > viniendo
    E N T I R	> -ENTIR, INTIENDO	# sentir > sintiendo
    E R T I R	> -ERTIR, IRTIENDO	# invertir > invirtiendo
    E R V I R	> -ERVIR, IRVIENDO	# hervir > hirviendo
    E S T I R	> -ESTIR, ISTIENDO	# vestir > vistiendo
    E G U I R	> -EGUIR, IGUIENDO	# seguir > siguiendo
    E R I R	> -ERIR, IRIENDO	# herir > hiriendo
    E T I R	> -ETIR, ITIENDO	# derretir > derritiendo
    I B I R	> -R, ENDO		# escribir > escribiendo
    O 'I R	> -'IR, YENDO		# o'ir > oyendo
    O R I R	> -ORIR, URIENDO	# morir > muriendo
    O R M I R	> -ORMIR, URMIENDO	# dormir > durmiendo
    U B R I R	> -R, ENDO		# cubrir > cubriendo
    [^G] U I R	> -IR, YENDO		# huir > huyendo
    G \"U I R	> -\"UIR, UYENDO	# arg"uir > arguyendo
    U L L I R	> -IR, ENDO		# mullir > mullendo
    U 'N I R	> -IR, ENDO		# gruñir > gruñendo
    E R G U I R	> -ERGUIR, IRGUIENDO	# erguir > irguiendo

# FINAL de las conjugaciones verbales irregulares.

flag *O:        # Verbos pronominales irregulares en el gerundio
