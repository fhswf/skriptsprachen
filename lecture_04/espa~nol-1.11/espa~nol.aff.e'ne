    [^A] N D A R> -R, 'ASEMOS		# recomendar > recomendases o recomendaras
    [^A] N D A R> -AR, 'ARAMOS
    [^N] D A R	> -AR, 'ASEMOS		# recordar > recordases o recordaras
    [^N] D A R 	> -AR, 'ARAMOS
    [^D] A R	> -AR, 'ASEMOS		# amar > am'asemos o am'aramos
    [^D] A R 	> -AR, 'ARAMOS
    A B E R	> -ABER, UPI'ERAMOS	# caber > cupiera o cupiese
    A B E R	> -ABER, UPI'ESEMOS
    [^FH] A C E R > -ER, I'ESEMOS	# nacer > naci'eramos o naci'esemos
    [^FH] A C E R > -ER, I'ERAMOS
    [FH] A C E R > -ACER, ICI'ERAMOS	# hacer > hici'eramos o hici'esemos
    [FH] A C E R > -ACER, ICI'ESEMOS
    A L E R	> -ER, I'ESEMOS		# valer > vali'eramos o vali'esemos
    A L E R	> -ER, I'ERAMOS
    C A E R	> -ER, Y'ESEMOS		# caer > cay'esemos o cay'eramos
    C A E R	> -ER, Y'ERAMOS
    E [CD] E R	> -ER, I'ESEMOS		# parecer > pareci'eramos o pareci'esemos
    E [CD] E R	> -ER, I'ERAMOS 
    [EO] E R	> -ER, Y'ESEMOS		# leer > ley'esemos o ley'eramos
    [EO] E R	> -ER, Y'ERAMOS
    E N D E R	> -ER, I'ESEMOS		# tender > tendi'eramos o tendi'esemos
    E N D E R	> -ER, I'ERAMOS
    E N E R	> -ENER, UVI'ESEMOS	# tener > tuvi'esemos o tuvi'eramos
    E N E R	> -ENER, UVI'ERAMOS
    E R [DNT] E R > -ER, I'ERAMOS	# perder > perdi'eramos o perdi'esemos
    E R [DNT] E R > -ER, I'ESEMOS
    E R E R	> -ERER, ISI'ESEMOS	# querer > quisi'esemos o quisi'eramos
    E R E R	> -ERER, ISI'ERAMOS
    O [CLV] E R	> -ER, I'ERAMOS		# conocer > conoci'eramos o conoci'esemos
    O [CLV] E R	> -ER, I'ESEMOS
    O D E R	> -ODER, UDI'ERAMOS	# poder > pudi'eramos o pudi'esemos
    O D E R	> -ODER, UDI'ESEMOS
    O L V E R	> -ER, I'ERAMOS		# volver > volvi'eramos o volvi'esemos
    O L V E R	> -ER, I'ESEMOS
    O N E R	> -ONER, USI'ESEMOS	# poner > pusi'esemos o pusi'eramos
    O N E R	> -ONER, USI'ERAMOS
    O R [CD] E R > -ER, I'ERAMOS	# torcer > torci'eramos o torci'esemos
    O R [CD] E R > -ER, I'ESEMOS
    R A E R	> -ER, J'ESEMOS		# traer > traj'esemos o traj'eramos
    R A E R	> -ER, J'ERAMOS
    A L I R	> -R, 'ESEMOS		# salir > sali'eramos o sali'esemos
    A L I R	> -R, 'ERAMOS
    E B I R	> -EBIR, IBI'ESEMOS	# concebir > concibi'eramos o concibi'esemos
    E B I R	> -EBIR, IBI'ERAMOS
    E C I R	> -ECIR, IJ'ESEMOS	# decir > dij'esemos o dij'eramos
    E C I R	> -ECIR, IJ'ERAMOS
    E D I R	> -EDIR, IDI'ESEMOS	# pedir > pidi'eramos o pidi'esemos
    E D I R	> -EDIR, IDI'ERAMOS
    E G I R	> -EGIR, IGI'ESEMOS	# elegir > elegi'eramos o elegi'esemos
    E G I R	> -EGIR, IGI'ERAMOS
    E M I R	> -EMIR, IMI'ESEMOS	# gemir > gimi'eramos o gimi'esemos
    E M I R	> -EMIR, IMI'ERAMOS
    E 'I R	> -E'IR, I'ESEMOS	# re'ir > rieras o rieses
    E 'I R	> -E'IR, I'ERAMOS
    E 'N I R	> -E'NIR, I'N'ESEMOS	# ce'nir > ci'n'eramos o ci'n'esemos
    E 'N I R	> -E'NIR, I'N'ERAMOS
    E G U I R	> -EGUIR, IGUI'ESEMOS	# seguir > sigui'eramos o sigui'esemos
    E G U I R	> -EGUIR, IGUI'ERAMOS
    E N C H I R	> -ENCHIR, INCHI'ERAMOS	# henchir > hinchi'eramos, hinchi'esemos
    E N C H I R	> -ENCHIR, INCHI'ESEMOS
    E N D I R	> -ENDIR, INDI'ESEMOS	# rendir > rindi'eramos o rindi'esemos
    E N D I R	> -ENDIR, INDI'ERAMOS
    E N I R	> -ENIR, INI'ESEMOS	# venir > vini'esemos o vini'eramos
    E N I R	> -ENIR, INI'ERAMOS
    E N T I R	> -ENTIR, INTI'ERAMOS	# sentir > sinti'eramos o sinti'esemos
    E N T I R	> -ENTIR, INTI'ESEMOS
    E R I R	> -ERIR, IRI'ERAMOS	# herir > hiri'eramos o hiri'esemos
    E R I R	> -ERIR, IRI'ESEMOS
    E R N I R	> -R, 'ESEMOS		# discernir > discerni'eramos
    E R N I R	> -R, 'ERAMOS		# o discerni'esemos
    E R T I R	> -ERTIR, IRTI'ERAMOS	# invertir > invirti'eramos
    E R T I R	> -ERTIR, IRTI'ESEMOS	# invertir > invirti'esemos
    E R V I R	> -ERVIR, IRVI'ERAMOS	# servir > sirvi'eramos
    E R V I R	> -ERVIR, IRVI'ESEMOS	# servir > sirvi'esemos
    E S T I R	> -ESTIR, ISTI'ERAMOS	# vestir > visti'eramos
    E S T I R	> -ESTIR, ISTI'ESEMOS	# vestir > visti'esemos
    E U N I R	> -R, 'ESEMOS		# reunir > reuni'eramos o reuni'esemos
    E U N I R	> -R, 'ERAMOS
    E T I R	> -ETIR, ITI'ERAMOS	# derretir > derriti'eramos
    E T I R	> -ETIR, ITI'ESEMOS	# derretir > derriti'esemos
    I R I R	> -IRIR, IRI'ERAMOS	# adquirir > adquiri'eramos
    I R I R	> -IRIR, IRI'ESEMOS	# adquirir > adquiri'esemos
    O H I B I R > -R, 'ESEMOS		# prohibir > prohibi'esemos o prohibi'eramos
    O H I B I R > -R, 'ERAMOS
    O 'I R	> -'IR, Y'ERAMOS	# o'ir > oy'eramos u oy'esemos
    O 'I R	> -'IR, Y'ESEMOS
    O R I R	> -ORIR, URI'ERAMOS	# morir > muri'eramos
    O R I R	> -ORIR, URI'ESEMOS	# morir > muri'esemos
    O R M I R	> -ORMIR, URMI'ERAMOS	# dormir > durmi'eramos
    O R M I R	> -ORMIR, URMI'ESEMOS	# dormir > durmi'esemos
    D U C I R	> -CIR, J'ERAMOS	# producir > produj'eramos o produj'esemos
    D U C I R	> -CIR, J'ESEMOS
    [^D] U C I R > -R, 'ERAMOS		# lucir > lucier'amos o lucies'emos
    [^D] U C I R > -R, 'ESEMOS
    [^G] U I R	> -IR, Y'ERAMOS		# huir > huy'eramos o huy'esemos
    [^G] U I R	> -IR, Y'ESEMOS
    G \"U I R	> -\"UIR, UY'ESEMOS	# arg"uir > arguy'eramos o arguy'esemos
    G \"U I R	> -\"UIR, UY'ERAMOS
    U L L I R	> -IR, 'ERAMOS		# mullir > mull'eramos o mull'esemos
    U L L I R	> -IR, 'ESEMOS
    U 'N I R	> -IR, 'ERAMOS		# gruñir > gruñeramos o gruñesemos
    U 'N I R	> -IR, 'ESEMOS
    E H U N D I R > -R, 'ERAMOS		# rehundir > rehundi'eramos o rehundi'esemos
